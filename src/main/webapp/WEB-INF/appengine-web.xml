<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>crosstalkchat</application>
  <version>3</version>
  <system-properties>
    <property name="mvel2.disable.jit" value="true"/>
    <property name="twittermode" value="false"/> <!-- Uses Google Buzz if false -->
    <property name="supersecure" value="true"/> <!-- restricts anonymous if true -->
    <property name="crosstalk.domain" value="http://crosstalkchat.appspot.com"/>
  </system-properties>
  <sessions-enabled>true</sessions-enabled>
</appengine-web-app>
